{
  "name": "pluralsight-js-dev-env",
  "version": "1.0.0",
  "description": "JavaScript Development Environment from Pluralsight Course with Cory House",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "prestart": "babel-node buildScripts/javascript/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node buildScripts/javascript/srcServer.js",
    "lint": "esw webpack.config.*, src, buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zivsof/pluralsight-js-dev-env.git"
  },
  "author": "Ziv Sofrin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zivsof/pluralsight-js-dev-env/issues"
  },
  "homepage": "https://github.com/Zivsof/pluralsight-js-dev-env#readme",
  "dependencies": {
    "ts-node-dev": "^1.0.0-pre.44",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "babel-loader": "8.0.6",
    "@types/express": "^4.17.4",
    "@types/node": "^13.11.0",
    "@types/open": "^6.2.1",
    "@types/webpack": "^4.41.10",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.7.4",
    "cross-env": "3.1.3",
    "css-loader": "^3.5.1",
    "eslint": "3.8.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "^4.0.4",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.16.1",
    "localtunnel": "^2.0.0",
    "lodash": "^4.17.15",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "npm-run-all": "^4.1.5",
    "nsp": "^3.2.1",
    "numeral": "^2.0.6",
    "open": "^7.0.3",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "surge": "0.20.4",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "production": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "es2015": {
                "modules": false
              }
            }
          ]
        ]
      }
    }
  }
}
